        .menu__item {
            --clip: polygon(0 0, 100% 0, 100% 0, 0 0);
            --transform: rotate(-90deg);
        }

            .menu__item:hover {
                --clip: polygon(0 0, 100% 0, 100% 100%, 0 100%);
                --transform: rotate(0);
            }

        .menu__item-redes {
            --clip: polygon(0 0, 0 0, 0 100%, 0 100%);
            --transform: rotate(-90deg);
            display: flex !important;
            align-items: center !important;
        }

            .menu__item-redes:hover {
                --clip: polygon(0 0, 100% 0, 100% 100%, 0 100%);
                --transform: rotate(0);
            }

        .activarDesplegableMenu {
            --clip: polygon(0 0, 100% 0, 100% 100%, 0 100%) !important;
            --transform: rotate(0) !important;
        }

        .menu__nesting {
            list-style: none;
            transition: clip-path .3s;
            clip-path: var(--clip);
            position: absolute;
            left: 0;
            bottom: -2px;
            width: max-content;
            transform: translateY(100%);
            background-color: var(--Primary-green);
            padding-bottom: 5px;
        }

            .menu__nesting li {
                padding: 5px 5px 2px 5px;
                padding: 5px 5px 2px 5px;
                transition: 0.3s ease all;
            }

                .menu__nesting li:not(.ocultar):hover {
                    border-bottom: 1px solid var(--Intense-green);
                    transform: scale(1.01);
                    background-color: #29b844;
                }

                .menu__nesting li:hover a {
                    color: #fff;
                }

            .menu__nesting a {
                font-size: 12px;
                font-weight: 400;
                color: #D0D0D0;
                padding: 5px;
                transition: 0.3s;
                transition-property: color;
            }

        .menu__nesting-redes {
            list-style: none;
            transition: clip-path .3s;
            clip-path: var(--clip);
            position: absolute;
            left: 50%;
            top: 50%;
            width: max-content;
            transform: translate(18%, -50%);
            border-radius: 5px;
            background-color: #fff;
            padding-bottom: 2px;
            display: flex;
            align-items: center;
            z-index: 9;
            border: 1px solid var(--Primary-green);
            /*display: grid;
                position: unset;
                width: 100%;
                transform: translateY(0);
                background-color: white;
                max-height: 0;
                transition: max-height .3s, height .3s;
                padding-bottom: 0;*/
        }

            .menu__nesting-redes li {
                padding: 5px 5px 2px 5px;
                padding: 5px 5px 2px 5px;
                transition: 0.3s ease all;
                /*border-left: 1px solid #fff;
                    width: 100%;
                    margin-left: auto;
                    padding: 0;*/
            }

            .menu__nesting-redes > li:hover {
                border-bottom: none;
            }

            .menu__nesting-redes > li > a {
                margin: 0px;
                color: #fff;
            }

            .menu__nesting-redes a {
                font-size: 12px;
                font-weight: 400;
                color: #D0D0D0;
                padding: 5px;
                transition: 0.3s;
                transition-property: color;
            }






        .menu__nestingCerrar {
            list-style: none;
            transition: clip-path .3s;
            clip-path: var(--clip);
            position: absolute;
            right: 0;
            bottom: 0;
            left: auto;
            width: 282px;
            transform: translateY(100%);
            background-color: #fff;
            padding-bottom: 5px;
            border-radius: 7px;
            box-shadow: inset rgba(0, 0, 0, 0.1) 0px 0px 2px 1px;
        }

