                } else {
                    $('#back-to-top').fadeOut();
                }
            });
            // scroll body to 0px on click
            $('#back-to-top').click(function () {
                $('body,html').animate({
                    scrollTop: 0
                }, 400);
                return false;
            });
        });


        // Función que se ejecutará cada 5 minutos
        function checkSession() {
            // Realizas la lógica AJAX para validar la sesión
            $.ajax({
                type: "POST",
                url: "Autocompletado.asmx/ValidateSession", // Ruta de tu WebMethod
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (result) {
                    if (!result.d) { // Asumiendo que el WebMethod devuelve { d: true/false }
                        window.location.href = "Login.aspx";  // Redirigir al login si la sesión no es válida
                    }
                },
                error: function (error) {
                    console.error("Error al verificar la sesión:", error);
                }
            });
        }

        // Ejecutar la función cada 5 minutos (180000 ms)
        setInterval(checkSession, 300000);

