            --Primary-purple: #B23F8E;
            --Medium-purple: #C971A8;
            --Light-purple: #D89FC6;
            /* Red */
            --Intense-red: #B31A1A;
            --Dark-red: #C11E1E;
            --Primary-red: #E42320;
            --Medium-red: #EB5A5A;
            --Light-red: #F19193;
            /* Green */
            --Intense-green: #17732A;
            --Dark-green: #198F34;
            --Primary-green: #1FA438;
            --Medium-green: #5FAF6B;
            --Light-green: #91D19C;
            /*Primary*/
